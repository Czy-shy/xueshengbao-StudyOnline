  margin-top: 5%;
  color: #118fff;
}

.feedback-form {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.content {
  height: 150px;
  width: 92%;
  margin: 2%;
  padding: 2%;
  white-space: pre-wrap;
  background: whitesmoke;
  color: #118fff;
  border-radius: 3%;
